doctype html
html(lang="en")
	include ../../common-html/head
	body
		table
			thead
				th ID
				th Shallow ID
				th Name
				th Class Name
				th Project
				th Created By
				th JSON
				th Created On
				th Last Modified
			each asset, name in datums
				tr
					td #{asset._id}
					td #{asset.shallowId}
					td #{asset.name}
					td #{asset.className}
					td #{asset.project_id}
					td #{asset.createdBy}
					td !{ JSON.stringify( asset.json )}
					td !{new Date(asset.created_on).toString()}
					td !{new Date(asset.last_modified).toString()}